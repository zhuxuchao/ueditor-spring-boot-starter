package com.maoface.ueditor.util;

/**
 * @author zhuxuchao
 * @version 1.0
 * @since 2022/7/15
 */
public class BeanUtils {

    public static <S, T> T convert(S source, Class<T> tagert){
        return null;
    }
    
    public static Set<String> findFieldNames(Class clazz) {
        Field[] fields = clazz.getDeclaredFields();
        Set<String> fieldNames = Arrays.stream(fields).map(Field::getName).collect(Collectors.toSet());
        return fieldNames;
    }
}
